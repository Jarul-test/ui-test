{
  "name": "ui-style-lib",
  "version": "1.0.2",
  "description": "A simple React component library using Next.js and TypeScript",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:js": "tsup",
    "build:css": "npx tailwindcss -i ./src/styles/tailwind.css -o ./dist/styles.css --minify",
    "build": "npm run build:js && npm run build:css",
    "lint": "next lint"
  },
  "peerDependencies": {
    "next": "15.1.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "tsup": "^8.3.6",
    "typescript": "^5"
  },
  "keywords": [
    "react",
    "nextjs",
    "component",
    "ui-library"
  ],
  "author": "Jarul",
  "dependencies": {
    "autoprefixer": "^10.4.20",
    "esbuild-style-plugin": "^1.6.3"
  }
}
